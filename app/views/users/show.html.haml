.text_color
  .col-md-3
    %h1 portfolio quotes
    .jumbotron.color_mellow
      = render 'quotes/form'
      %table
        %thead
          %th
        %tbody
        - @quotes.each do |q|
          %tr
            %td= q.ticker

  .col-md-4
    %h1 profile
    .jumbotron.color_mellow
      = render 'form_profile'
